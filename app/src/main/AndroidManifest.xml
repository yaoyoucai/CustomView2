<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="shbd.customview">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.SetPolyToPolyActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.CheckButtonActivity" />
        <activity android:name=".activity.DrawTextActivity" />
        <activity android:name=".activity.DrawBessellActivity" />
        <activity android:name=".activity.DrawPathActivity" />
        <activity android:name=".activity.DrawMatrixActivity" />
    </application>

</manifest>